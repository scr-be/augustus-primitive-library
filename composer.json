{
    "name"        : "scr-be/augustus-primitive-library",
    "license"     : "MIT",
    "type"        : "library",
    "homepage"    : "https://scr.be/augustus-primitive-library/home",
    "description" : "Implementations of various collection-types to augment the lack of array-like primitives in PHP.",
    "support" : {
        "email"  : "rmf@scr.be",
        "source" : "https://scr.be/augustus-primitive-library/git",
        "issues" : "https://scr.be/augustus-primitive-library/issues",
        "irc"    : "irc://irc.efnet.org/scribe"
    },
    "keywords"    : [
        "augustus-primitive-library", "scribe-augustus-primitive-library", "library", "scr.be", "scribe", "scribenet", "scribe-softwade", "scribe-services"
    ],
    "authors" : [
        {
            "name"     : "Rob Frawley 2nd",
            "email"    : "rmf@scr.be",
            "homepage" : "https://scr.be/rmf",
            "role"     : "Project Lead"
        }
    ],
    "require" : {
        "php" : ">=5.6.4,~5.6|~7.0"
    },
    "require-dev" : {
        "phpunit/phpunit"        : "~5.0",
        "scrutinizer/ocular"     : "~1.1",
        "satooshi/php-coveralls" : "~0.7@dev"
    },
    "autoload" : {
        "psr-4" : {
            "Scribe\\Primitive\\" : "lib/"
        }
    },
    "autoload-dev" : {
        "psr-4" : {
            "Scribe\\Primitive\\Tests\\" : "tests/"
        }
    },
    "prefer-stable"     : true,
    "minimum-stability" : "dev",
    "extra" : {
        "branch-alias" : {
            "dev-master" : "0.1.x-dev"
        }
    }
}
